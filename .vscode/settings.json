{
  "cSpell.words": [
    "composables",
    "daterange",
    "datetime",
    "domexception",
    "esbuild",
    "excelize",
    "fatih",
    "gonic",
    "GOPROXY",
    "gorm",
    "jsoniter",
    "mapstructure",
    "Milli",
    "mitchellh",
    "nuxt",
    "panigation",
    "pinia",
    "primevue",
    "Rapboy",
    "samber",
    "testdata",
    "tsdoc",
    "tsunion",
    "Upsert",
    "xuri"
  ],
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "npm.packageManager": "pnpm",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[go.mod]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.schema": "jsonc",
    "*.toml": "toml", // 修改这里，将所有.toml文件关联到toml语言模式
    ".air.toml": "toml" // 新增这一行，将.air.toml文件关联到toml语言模式
  },
  "material-icon-theme.folders.associations": {
    "vue/composables": "shared",
    ".build/.server": "temp",
    ".build/production": "next",
    "server/v2": "server"
  },
  "material-icon-theme.files.associations": {
    "pack.config.json": "zip",
    "server.config.json": "settings"
  },
  "json.schemas": [
    {
      "fileMatch": ["/server.config.json"],
      "url": "/node_modules/nuxt-gin-tools/src/server-config.json"
    },
    {
      "fileMatch": ["/pack.config.json"],
      "url": "/node_modules/nuxt-gin-tools/src/pack-config.schema.json"
    }
  ],
  "python.analysis.typeCheckingMode": "basic",
  "workbench.iconTheme": "material-icon-theme"
}
